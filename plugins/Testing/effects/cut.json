{
  "name": "Cut",
  "description": "Shorten the video by cutting it.",
  "color": "E27C54",
  "magic": "-ss {start_timestamp} -to {stop_timestamp} -c copy",
  "controls": [{
    "name": "Start Timestamp",
    "magic_name": "start_timestamp",
    "description": "Start at...",
    "type": "TimeProperty"
  },
  {
    "name": "Stop Timestamp",
    "magic_name": "stop_timestamp",
    "description": "Stop at...",
    "type": "TimeProperty"
  }]
}
